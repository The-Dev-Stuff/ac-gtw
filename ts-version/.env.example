# AWS Configuration
AWS_DEFAULT_REGION=us-east-1

# Server Configuration
PORT=3000
NODE_ENV=development

# OpenAPI Specs Bucket (required - will auto-generate with account ID if not provided)
OPENAPI_SPECS_BUCKET=agentcore-gateway-openapi-specs-bucket

# Gateway Configuration (required)
GATEWAY_ROLE_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:role/sample-lambdagateway-role-demo

# Optional: External API Keys for testing
# NASA_API_KEY=your_nasa_key_here
# OTHER_API_KEY=your_other_key_here

