		{
			"name": "Create Tool from inline OpenAPI Spec",
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"gateway_id\": \"samplegatewaysecond-s8qkj5fs2n\",\n    \"tool_name\": \"testing-todos-three\",\n    \"openapi_spec\": {\n    \"openapi\": \"3.0.3\",\n    \"info\": {\n        \"title\": \"test-sample-todos-three\",\n        \"description\": \"Sample API with todos, samples, and test-data endpoints\",\n        \"version\": \"1.0.0\"\n    },\n    \"servers\": [\n        {\n            \"url\": \"https://dummyjson.com\"\n        }\n    ],\n    \"paths\": {\n        \"/samples\": {\n            \"get\": {\n                \"summary\": \"Get all samples\",\n                \"operationId\": \"get-samples\",\n                \"responses\": {\n                    \"200\": {\n                        \"description\": \"Successful response\"\n                    }\n                }\n            },\n            \"put\": {\n                \"summary\": \"Update a sample\",\n                \"operationId\": \"update-sample\",\n                \"responses\": {\n                    \"200\": {\n                        \"description\": \"Sample updated successfully\"\n                    }\n                }\n            }\n        },\n        \"/test-data\": {\n            \"get\": {\n                \"summary\": \"Get test data\",\n                \"operationId\": \"get-test-data\",\n                \"responses\": {\n                    \"200\": {\n                        \"description\": \"Successful response\"\n                    }\n                }\n            }\n        }\n    }\n}\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://localhost:8000/tools/from-spec",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8000",
					"path": [
						"tools",
						"from-spec"
					]
				}
			},
			"response": []
		},
